// Source: https://usaco.guide/general/io

#include <bits/stdc++.h>
using namespace std;

int main() {
vector<int> v;
for (int i = 0; i < 7; ++i) {
        int num;
        std::cin >> num;
        v.push_back(num);
    }
sort(v.begin(), v.end());
int a = v[0];
int b = v[1];
int c = v[6] - a - b;
cout << a << " " << b << " " << c;
}
